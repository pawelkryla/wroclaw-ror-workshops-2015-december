= simple_form_for student do |f|
  = f.input :first_name
  = f.input :last_name
  - if student_subject_items.present?
    = f.association :student_subject_items, label_method: :title, value_method: :id, label: 'Subject Items', collection: subject_items, as: :check_boxes
  = link_to t('shared.back'), students_path, class: 'btn btn-default'
  = f.button :submit
